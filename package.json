{
  "name": "ttn-stack",
  "version": "3.0.0-pre",
  "description": "The Thing Network Stack",
  "main": "index.js",
  "repository": "https://github.com/TheThingsNetwork/ttn.git",
  "license": "Apache-2.0",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/react": "^3.2.16",
    "add-asset-html-webpack-plugin": "^2.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-react-intl-auto": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "eslint": "^4.19.1 ",
    "eslint-config-ttn": "^1.3.5",
    "css-loader": "^0.28.11",
    "eslint": "^4.11.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^21.2.1",
    "js-yaml": "^3.10.0",
    "json": "^9.0.6",
    "json-loader": "^0.5.7",
    "markup-inline-loader": "^0.2.2",
    "messageformat-parser": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-localstorage": "^1.3.0",
    "nsp": "^3.1.0",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "tcomb": "^3.2.24",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.14",
    "webpack-livereload-plugin": "^2.1.1",
    "webpack-subresource-integrity": "^1.0.1",
    "xlsx-style": "^0.8.13"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-decorators-legacy",
      "transform-react-constant-elements",
      "transform-react-inline-elements",
      "transform-runtime",
      [
        "react-intl-auto",
        {
          "extractComments": true,
          "filebase": true,
          "removePrefix": "pkg.client"
        }
      ],
      [
        "react-intl",
        {
          "messagesDir": "./.cache/messages",
          "extractSourceLocation": true
        }
      ]
    ]
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "babel-polyfill": "^6.26.0",
    "cancelable-promise": "^2.4.0",
    "classnames": "^2.2.5",
    "event-emitter": "^0.3.5",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "intl": "^1.2.5",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-display-name": "^0.2.3",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-select": "^1.0.0-rc.10",
    "react-side-effect": "^1.1.3",
    "react-text-mask": "^5.0.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-form": "^7.2.1",
    "redux-saga": "^0.16.0",
    "sockjs-client": "^1.1.4",
    "ulid": "^2.3.0",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/pkg/**/*_test.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(css|styl|less|sass)$": "<rootDir>/config/jest/styles.transform.js",
      "\\.(jpg|jpeg|svg|png|woff|woff2|ttf|eot|gif|webp)$": "<rootDir>/config/jest/files.transform.js"
    }
  },
  "eslintConfig": {
    "extends": "./config/eslintrc.yaml"
  }
}
